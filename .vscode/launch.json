    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/server",
            "args": ["5000", "./", "2", "4"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {"text": "-gdb-set follow-fork-mode child"}
            ]
        },


    

        
        {
            "name": "(gdb) Launch2",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/client",
            "args": ["localhost", "5000", "3", "CONCUR", "/index.html"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {"text": "-gdb-set follow-fork-mode child"}
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}